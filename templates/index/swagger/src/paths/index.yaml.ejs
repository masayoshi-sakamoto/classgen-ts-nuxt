<%_ for (const path of paths['src/paths'].dirs) { _%>
#<%= upperCamel(path.name) %>
<%_ for (const file of path.files) { _%>
<%_ const filemame = file.split('.')[0] _%>
  <%_ if (filemame === 'path') { _%>
/<%= path.name %>/{id}:
  <%_ } else if (filemame === 'paths') { _%>
/<%= path.name %>:
  <%_ } else { _%>
/<%= path.name %>/<%= filemame %>:
  <%_ } _%>
  $ref: './<%= path.name %>/<%= file %>'
<%_ } _%>
  
<%_ } _%>